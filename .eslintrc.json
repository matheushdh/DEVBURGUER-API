{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-unused-vars": "off",
    "no-console":"off",
    "no-use-before-define": "off",
    "no-param-reassign": "off",
    "no-undef":"off",
    "no-template-curly-in-string":"off",
    "import/no-unresolved":"off",
    "consistent-return":"off"
  }
}
